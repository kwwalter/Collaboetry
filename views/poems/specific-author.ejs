<h1 class="poems-by-H1">All poems by <%= authorName %></h1>
<!-- TO-DO: link up Users so we can display the user's name in this h1 -->

<% poems.forEach(function(poem){ %>
  <div class="poems-by-author">
    <!-- <h2><%= poem.username %></h2> -->
  <h2><a href="/poems/authors/<%= poem.poetID %>/<%= poem._id %>"><%= poem.title || "untitled" %></a></h2>
  </div>
<% }); %>
